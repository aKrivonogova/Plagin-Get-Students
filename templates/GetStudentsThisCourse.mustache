{{#isUserListNotEmpty}}
<section class="students-by-group">
    <div class="students-list-container">
        <h4 class="students-list-title">
            Студенты
        </h4>
        <ul class="students-list">
            {{#usersListOutput}}
            <li class="student-list-item" data-id="{{id}}"> {{firstname}} {{lastname}}</li>
            {{/usersListOutput}}
        </ul>
    </div>
    <div class="student-item-container">
        <div class="student-item-image">
            {{#hasFirstPicture}}
            <img src="{{firstPicture}}" alt="">
            {{/hasFirstPicture}}
            {{^hasFirstPicture}}
            <svg fill="#000000" width="50px" height="50px" viewBox="0 0 16 16" xmlns="http://www.w3.org/2000/svg">
                <g>
                    <path
                        d="M10.31,9.12H5.5A4.52,4.52,0,0,0,1,13.62,2.34,2.34,0,0,0,1,14H14.78a2.34,2.34,0,0,0,0-.38A4.51,4.51,0,0,0,10.31,9.12ZM8,7.88A3.94,3.94,0,1,0,4.06,3.94,3.94,3.94,0,0,0,8,7.88Z" />
                </g>
            </svg>
            {{/hasFirstPicture}}
        </div>
        <h4 class="students-item-username">
            {{firstname}}
        </h4>
        <h4 class="students-item-username">
            {{lastname}}
        </h4>
    </div>
</section>
{{/isUserListNotEmpty}}
<div class="message">
    {{^isUserListNotEmpty}}
    В этой группе еще нет студентов
    {{/isUserListNotEmpty}}
</div>